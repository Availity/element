{
  "name": "design-tokens",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/design-tokens/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "eslintConfig": ".eslintrc.json",
        "silent": false,
        "fix": false,
        "cache": true,
        "cacheLocation": "./node_modules/.cache/design-tokens/.eslintcache",
        "maxWarnings": -1,
        "quiet": false,
        "noEslintrc": false,
        "hasTypeAwareRules": true,
        "cacheStrategy": "metadata"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/design-tokens"],
      "options": {
        "jestConfig": "packages/design-tokens/jest.config.js"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "commitMessageFormat": "chore({projectName}): release version ${version} [skip ci]",
        "tagPrefix": "@availity/{projectName}@",
        "trackDeps": true
      }
    }
  }
}
