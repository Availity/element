{
  "name": "@availity/element",
  "version": "0.31.2",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "dependencies": {
    "@availity/mui-alert": "workspace:*",
    "@availity/mui-autocomplete": "workspace:*",
    "@availity/mui-avatar": "workspace:*",
    "@availity/mui-backdrop": "workspace:*",
    "@availity/mui-badge": "workspace:*",
    "@availity/mui-breadcrumbs": "workspace:*",
    "@availity/mui-button": "workspace:*",
    "@availity/mui-card": "workspace:*",
    "@availity/mui-checkbox": "workspace:*",
    "@availity/mui-chip": "workspace:*",
    "@availity/mui-datepicker": "workspace:*",
    "@availity/mui-divider": "workspace:*",
    "@availity/mui-feedback": "workspace:*",
    "@availity/mui-form-utils": "workspace:*",
    "@availity/mui-icon": "workspace:*",
    "@availity/mui-layout": "workspace:*",
    "@availity/mui-link": "workspace:*",
    "@availity/mui-menu": "workspace:*",
    "@availity/mui-modal": "workspace:*",
    "@availity/mui-page-header": "workspace:*",
    "@availity/mui-pagination": "workspace:*",
    "@availity/mui-paper": "workspace:*",
    "@availity/mui-popover": "workspace:*",
    "@availity/mui-progress": "workspace:*",
    "@availity/mui-table": "workspace:*",
    "@availity/mui-tabs": "workspace:*",
    "@availity/mui-textfield": "workspace:*",
    "@availity/mui-toggle-button": "workspace:*",
    "@availity/mui-tooltip": "workspace:*",
    "@availity/mui-typography": "workspace:*",
    "@availity/theme-provider": "workspace:*",
    "@mui/material": "^5.14.12",
    "@mui/system": "^5.14.12"
  },
  "devDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^5.12.7",
    "typescript": "~4.8.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
