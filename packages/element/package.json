{
  "name": "@availity/element",
  "version": "0.7.0",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "dependencies": {
    "@availity/mui-alert": "workspace:*",
    "@availity/mui-badge": "workspace:*",
    "@availity/mui-divider": "workspace:*",
    "@availity/mui-icon": "workspace:*",
    "@availity/mui-paper": "workspace:*",
    "@availity/mui-tooltip": "workspace:*",
    "@availity/theme-provider": "workspace:*",
    "@mui/material": "^5.11.9"
  },
  "devDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^5.12.7",
    "typescript": "~4.8.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
