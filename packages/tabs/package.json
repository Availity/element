{
  "name": "@availity/mui-tabs",
  "version": "1.0.1",
  "description": "Availity MUI Tabs Component - part of the @availity/element design system",
  "keywords": [
    "react",
    "typescript",
    "availity",
    "mui"
  ],
  "homepage": "https://availity.github.io/element/?path=/docs/components-tabs-introduction--docs",
  "bugs": {
    "url": "https://github.com/Availity/element/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Availity/element.git",
    "directory": "packages/tabs"
  },
  "license": "MIT",
  "author": "Availity Developers <AVOSS@availity.com>",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "clean": "rm -rf dist",
    "clean:nm": "rm -rf node_modules",
    "publish": "yarn npm publish --tolerate-republish --access public",
    "publish:canary": "yarn npm publish --access public --tag canary"
  },
  "dependencies": {
    "@mui/lab": "6.0.0-beta.12"
  },
  "devDependencies": {
    "@availity/mui-divider": "workspace:^",
    "@mui/material": "^6.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@mui/material": "^6.4.5",
    "react": ">=16.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
