{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictNullChecks": true,
    "lib": ["ES2017", "dom"],
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@availity/design-tokens": ["packages/design-tokens/src/index.ts"],
      "@availity/element": ["packages/element/src/index.ts"],
      "@availity/mock": ["packages/mock"],
      "@availity/mui-accordion": ["packages/accordion/src/index.ts"],
      "@availity/mui-alert": ["packages/alert/src/index.ts"],
      "@availity/mui-authorize": ["packages/authorize/src/index.ts"],
      "@availity/mui-autocomplete": ["packages/autocomplete/src/index.ts"],
      "@availity/mui-avatar": ["packages/avatar/src/index.ts"],
      "@availity/mui-backdrop": ["packages/backdrop/src/index.ts"],
      "@availity/mui-badge": ["packages/badge/src/index.ts"],
      "@availity/mui-block-ui": ["packages/block-ui/src/index.ts"],
      "@availity/mui-breadcrumbs": ["packages/breadcrumbs/src/index.ts"],
      "@availity/mui-button": ["packages/button/src/index.ts"],
      "@availity/mui-card": ["packages/card/src/index.ts"],
      "@availity/mui-checkbox": ["packages/checkbox/src/index.ts"],
      "@availity/mui-chip": ["packages/chip/src/index.ts"],
      "@availity/mui-codemod": ["packages/codemod/src/index.ts"],
      "@availity/mui-controlled-form": ["packages/controlled-form/src/index.ts"],
      "@availity/mui-datepicker": ["packages/datepicker/src/index.ts"],
      "@availity/mui-dialog": ["packages/dialog/src/index.ts"],
      "@availity/mui-disclaimer": ["packages/disclaimer/src/index.ts"],
      "@availity/mui-divider": ["packages/divider/src/index.ts"],
      "@availity/mui-drawer": ["packages/drawer/src/index.ts"],
      "@availity/mui-empty-state": ["packages/empty-state/src/index.ts"],
      "@availity/mui-event-tracker": ["packages/event-tracker/src/index.ts"],
      "@availity/mui-favorites": ["packages/favorites/src/index.ts"],
      "@availity/mui-feedback": ["packages/feedback/src/index.ts"],
      "@availity/mui-file-selector": ["packages/file-selector/src/index.ts"],
      "@availity/mui-form-utils": ["packages/form-utils/src/index.ts"],
      "@availity/mui-icon": ["packages/icon/src/index.ts"],
      "@availity/mui-layout": ["packages/layout/src/index.ts"],
      "@availity/mui-link": ["packages/link/src/index.ts"],
      "@availity/mui-list": ["packages/list/src/index.ts"],
      "@availity/mui-menu": ["packages/menu/src/index.ts"],
      "@availity/mui-modal": ["packages/modal/src/index.ts"],
      "@availity/mui-page-header": ["packages/page-header/src/index.ts"],
      "@availity/mui-pagination": ["packages/pagination/src/index.ts"],
      "@availity/mui-paper": ["packages/paper/src/index.ts"],
      "@availity/mui-popover": ["packages/popover/src/index.ts"],
      "@availity/mui-popper": ["packages/popper/src/index.ts"],
      "@availity/mui-progress": ["packages/progress/src/index.ts"],
      "@availity/mui-snackbar": ["packages/snackbar/src/index.ts"],
      "@availity/mui-spaces": ["packages/spaces/src/index.ts"],
      "@availity/mui-stepper": ["packages/stepper/src/index.ts"],
      "@availity/mui-table": ["packages/table/src/index.ts"],
      "@availity/mui-tabs": ["packages/tabs/src/index.ts"],
      "@availity/mui-textfield": ["packages/textfield/src/index.ts"],
      "@availity/mui-toggle-button": ["packages/toggle-button/src/index.ts"],
      "@availity/mui-tooltip": ["packages/tooltip/src/index.ts"],
      "@availity/mui-transitions": ["packages/transitions/src/index.ts"],
      "@availity/mui-tree": ["packages/tree/src/index.ts"],
      "@availity/mui-typography": ["packages/typography/src/index.ts"],
      "@availity/mui-utils": ["packages/utils/src/index.ts"],
      "@availity/theme": ["packages/theme/src/index.ts"],
      "@availity/theme-provider": ["packages/theme-provider/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"],
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
